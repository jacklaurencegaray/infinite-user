{
  "expo": {
    "name": "Infinite User",
    "description": "My First React Native App Ever.",
    "slug": "infinite-user-template",
    "privacy": "public",
    "sdkVersion": "31.0.0",
    "platforms": ["ios", "android"],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "android": {
      "package": "net.jacklaurence.infiniteuser"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "net.jacklaurence.infiniteuser"
    }
  }
}
